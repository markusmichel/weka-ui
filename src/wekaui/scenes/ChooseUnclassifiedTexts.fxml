<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wekaui.scenes.ChooseUnclassifiedTextsController">
   <children>
      <VBox minHeight="-Infinity" minWidth="-Infinity" stylesheets="@stylesheets/main.css">
         <children>
            <Label id="labelTitle" maxWidth="1.7976931348623157E308" text="Unklassifizierten Daten wählen" />
            <VBox>
               <children>
                  <Label fx:id="labelTexts" maxWidth="1.7976931348623157E308" styleClass="subheader" text="Model-Informationen">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="-10.0" />
               </VBox.margin>
            </VBox>
            <VBox>
               <children>
                  <Label id="labelOpenData" fx:id="labelOpenData" maxWidth="1.7976931348623157E308" styleClass="subheader" text="Unklassifizierte Daten öffnen">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <Label id="dropzoneData" fx:id="dropzoneData" alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="144.0" prefWidth="542.0" styleClass="light-border" text="Datei(en) hier reinziehen oder klicken zum Öffnen" textAlignment="CENTER">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </VBox>
         </children>
      </VBox>
      <Button fx:id="prevButton" mnemonicParsing="false" onAction="#onPrevClicked" text="zurück" />
   </children>
</StackPane>
